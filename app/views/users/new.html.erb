<h1> New User </h1>
<%= form_for @user do |f| %>
  <p>User name:<br><%= f.text_field :username %></p>
  <p>User email:<br><%= f.text_field  :email %></p>
  <p>User password:<br><%= f.password_field :password %></p>
  <p><%= f.submit "New user" %></p>
<% end %>
